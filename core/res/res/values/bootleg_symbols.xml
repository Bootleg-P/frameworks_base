<resources>

  <!-- Power menu -->
  <java-symbol type="string" name="system_error_vendorprint" />

  <!-- Device keyhandlers -->
  <java-symbol type="array" name="config_deviceKeyHandlerLibs" />
  <java-symbol type="array" name="config_deviceKeyHandlerClasses" />

  <!-- Usb drive persistent notification -->
  <java-symbol type="bool" name="config_persistUsbDriveNotification" />

  <!-- Rotation sensor -->
  <java-symbol type="bool" name="config_useSystemClockforRotationSensor" />

  <!-- Whether notify fingerprint client of successful cancelled authentication -->
  <java-symbol type="bool" name="config_notifyClientOnFingerprintCancelSuccess" />

  <!-- Package Manager -->
  <java-symbol type="array" name="config_disabledComponents" />
  <java-symbol type="array" name="config_forceEnabledComponents" />

  <!-- Whether device ignores the RSSNR signal implementation -->
  <java-symbol type="bool" name="config_ignoreRssnrSignalLevel" />

  <!-- Whether device has dash charging support -->
  <java-symbol type="bool" name="config_hasDashCharger" />

  <!-- Whether device has turbo power charging support -->
  <java-symbol type="bool" name="config_hasTurboPowerCharger" />

  <!-- Screenrecord -->
  <java-symbol type="bool" name="config_enableScreenrecordChord" />

  <!-- Power menu reboot -->
  <java-symbol name="reboot_to_bootloader_title" type="string" />
  <java-symbol name="reboot_to_bootloader_message" type="string" />
  <java-symbol name="reboot_to_recovery_title" type="string" />
  <java-symbol name="reboot_to_recovery_message" type="string" />
  <java-symbol name="reboot_system_title" type="string" />
  <java-symbol name="reboot_system_message" type="string" />
  <java-symbol type="array" name="config_rebootActionsList" />
  <java-symbol type="array" name="values_globalActionsList" />
  <java-symbol type="style" name="GlobalActionsAnimation" />
  <java-symbol type="style" name="GlobalActionsAnimationTop" />
  <java-symbol type="style" name="GlobalActionsAnimationEnter" />

  <!-- App error dialog -->
  <java-symbol type="id" name="aerr_copy" />
  <java-symbol type="string" name="url_copy_success" />
  <java-symbol type="string" name="url_copy_failed" />

  <!-- Bootleg unneccesary gradient declaration | Just for our shishu bars -->
  <java-symbol type="color" name="bootleg_accent_gradient_end_color" />

  <!-- Full screen aspect ratio -->
  <java-symbol type="bool" name="config_haveHigherAspectRatioScreen" />
  <java-symbol type="dimen" name="config_screenAspectRatio" />

  <!-- The list of vendor package signatures that should also be considered
    platform signatures, specifically for use on devices with a vendor partition. -->
  <java-symbol type="array" name="config_vendorPlatformSignatures" />

  <java-symbol type="integer" name="config_buttonBrightnessSettingDefault" />
  <java-symbol type="bool" name="config_deviceHasVariableButtonBrightness" />

  <!-- HW keys binding -->
  <java-symbol type="anim" name="custom_app_in" />
  <java-symbol type="anim" name="custom_app_out" />
  <java-symbol type="integer" name="config_deviceHardwareKeys" />

  <!-- System-wide recents task view roundness to apply to all the recents view.
       this value will apply on Launcher3 quickstep view and also the normal recents -->
  <java-symbol type="dimen" name="recents_task_view_rounded_corners_radius" />

  <!-- Oreo's recents component activity -->
  <java-symbol type="string" name="config_recentsComponentNameOreo" />

  <!-- Device LED -->
  <java-symbol type="bool" name="config_intrusiveBatteryLed" />
  <java-symbol type="bool" name="config_multiColorBatteryLed" />
  <java-symbol type="bool" name="config_ledCanPulse" />
  <java-symbol type="bool" name="config_multiColorNotificationLed" />

  <!-- Whether to cleanup fingerprints upon connection to the daemon and when user switches -->
  <java-symbol type="bool" name="config_cleanupUnusedFingerprints" />

  <!-- Weather icons -->
  <java-symbol type="drawable" name="weather_01" />
  <java-symbol type="drawable" name="weather_01n" />
  <java-symbol type="drawable" name="weather_02" />
  <java-symbol type="drawable" name="weather_02n" />
  <java-symbol type="drawable" name="weather_03" />
  <java-symbol type="drawable" name="weather_03n" />
  <java-symbol type="drawable" name="weather_04" />
  <java-symbol type="drawable" name="weather_04n" />
  <java-symbol type="drawable" name="weather_09" />
  <java-symbol type="drawable" name="weather_10" />
  <java-symbol type="drawable" name="weather_10n" />
  <java-symbol type="drawable" name="weather_11" />
  <java-symbol type="drawable" name="weather_13" />
  <java-symbol type="drawable" name="weather_50" />

  <!-- ADB notification -->
  <java-symbol type="string" name="adb_net_active_notification_title" />
  <java-symbol type="string" name="adb_both_active_notification_title" />
  <java-symbol type="string" name="adb_active_generic_notification_message" />

  <!-- EdgeGesture service -->
  <java-symbol type="dimen" name="edge_gesture_trigger_distance" />
  <java-symbol type="dimen" name="edge_gesture_perpendicular_distance" />
  <java-symbol type="dimen" name="edge_gesture_trigger_thickness" />

</resources>
